<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">

<listeners>
    <listener class-name="utils.MyListener"/>
  </listeners>
  
<!-- Pass browser parameter (chrome / firefox) -->
  <parameter name="browser" value="chrome"/>
  
  <test thread-count="5" name="Test">
    <classes>
      <class name="testcases.RegisterTest"/>
      <class name="testcases.LoginTest"/>
      <class name="testcases.LogoutTest"/>
      <class name="testcases.RegisterNegativeTest"/>
      <class name="testcases.LoginNegativeTests"/>
      <class name="testcases.BrowseNavigationTest"/>
      <class name="testcases.ProductCartTest"/>
      <class name="testcases.SearchFilterTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
